<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD
3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="po.ReleaseTest" table="question" catalog="termproject">
        <id name="qId" type="int">
            <column name="Qid" />
            <generator class="increment" />
        </id>
        <property name="tId" type="string">
            <column name="Tid" length="20" />
        </property>
        <property name="clId" type="string">
            <column name="Clid" length="20" />
        </property>
        <property name="context" type="string">
            <column name="Context" length="1000" />
        </property>
        <property name="type" type="string">
            <column name="Type" length="10" />
        </property>
        <property name="answer" type="string">
            <column name="Answer" length="100" />
        </property>
        <property name="startTime" type="java.util.Date">
            <column name="Start_time"  />
        </property>
        <property name="endTime" type="java.util.Date">
            <column name="End_time"  />
        </property>
    </class>
</hibernate-mapping>