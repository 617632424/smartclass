<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD
3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="po.Teacher" table="teacher" catalog="software">
        <id name="tid" type="java.lang.String">
            <column name="tid" length="12"/>
            <generator class="assigned" />
        </id>
        <property name="tname" type="string">
            <column name="tname" length="10" />
        </property>
    </class>
</hibernate-mapping>